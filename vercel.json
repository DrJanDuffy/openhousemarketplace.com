  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://openhousemarketplace.com",
    "SKIP_ENV_VALIDATION": "true"
